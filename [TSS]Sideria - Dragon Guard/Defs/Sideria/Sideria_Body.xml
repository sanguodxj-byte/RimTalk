<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Sideria 人形身体结构（带龙角和龙尾） -->
    <BodyDef>
        <defName>Sideria_Body</defName>
        <label>sideria body</label>
        <corePart>
            <!-- 躯干 - 核心 -->
            <def>Torso</def>
            <groups>
                <li>Torso</li>
            </groups>
            <parts>
                <!-- 脊椎 -->
                <li>
                    <def>Spine</def>
                    <coverage>0.03</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 胃 -->
                <li>
                    <def>Stomach</def>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 心脏 -->
                <li>
                    <def>Heart</def>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 左肺 -->
                <li>
                    <def>Lung</def>
                    <customLabel>left lung</customLabel>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 右肺 -->
                <li>
                    <def>Lung</def>
                    <customLabel>right lung</customLabel>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 左肾 -->
                <li>
                    <def>Kidney</def>
                    <customLabel>left kidney</customLabel>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 右肾 -->
                <li>
                    <def>Kidney</def>
                    <customLabel>right kidney</customLabel>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 肝脏 -->
                <li>
                    <def>Liver</def>
                    <coverage>0.02</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 颈部 -->
                <li>
                    <def>Neck</def>
                    <coverage>0.03</coverage>
                    <groups>
                        <li>Neck</li>
                    </groups>
                    <parts>
                        <!-- 头部 -->
                        <li>
                            <def>Head</def>
                            <coverage>0.8</coverage>
                            <groups>
                                <li>FullHead</li>
                                <li>UpperHead</li>
                            </groups>
                            <parts>
                                <!-- 头骨 -->
                                <li>
                                    <def>Skull</def>
                                    <coverage>0.1</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>UpperHead</li>
                                    </groups>
                                    <parts>
                                        <!-- 大脑 -->
                                        <li>
                                            <def>Brain</def>
                                            <coverage>0.25</coverage>
                                            <groups>
                                                <li>FullHead</li>
                                                <li>UpperHead</li>
                                            </groups>
                                        </li>
                                    </parts>
                                </li>
                                <!-- 左眼 -->
                                <li>
                                    <def>Eye</def>
                                    <customLabel>left eye</customLabel>
                                    <coverage>0.05</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>Eyes</li>
                                    </groups>
                                </li>
                                <!-- 右眼 -->
                                <li>
                                    <def>Eye</def>
                                    <customLabel>right eye</customLabel>
                                    <coverage>0.05</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>Eyes</li>
                                    </groups>
                                </li>
                                <!-- 左耳 -->
                                <li>
                                    <def>Ear</def>
                                    <customLabel>left ear</customLabel>
                                    <coverage>0.05</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>UpperHead</li>
                                    </groups>
                                </li>
                                <!-- 右耳 -->
                                <li>
                                    <def>Ear</def>
                                    <customLabel>right ear</customLabel>
                                    <coverage>0.05</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>UpperHead</li>
                                    </groups>
                                </li>
                                <!-- 鼻子 -->
                                <li>
                                    <def>Nose</def>
                                    <coverage>0.05</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                    </groups>
                                </li>
                                <!-- 下颌 -->
                                <li>
                                    <def>Jaw</def>
                                    <coverage>0.1</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                    </groups>
                                </li>
                                <!-- 左龙角 -->
                                <li>
                                    <def>Sideria_DragonHorn</def>
                                    <customLabel>left dragon horn</customLabel>
                                    <coverage>0.03</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>UpperHead</li>
                                    </groups>
                                </li>
                                <!-- 右龙角 -->
                                <li>
                                    <def>Sideria_DragonHorn</def>
                                    <customLabel>right dragon horn</customLabel>
                                    <coverage>0.03</coverage>
                                    <groups>
                                        <li>FullHead</li>
                                        <li>UpperHead</li>
                                    </groups>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <!-- 左肩 -->
                <li>
                    <def>Shoulder</def>
                    <customLabel>left shoulder</customLabel>
                    <coverage>0.04</coverage>
                    <groups>
                        <li>Shoulders</li>
                    </groups>
                    <parts>
                        <!-- 左臂 -->
                        <li>
                            <def>Arm</def>
                            <customLabel>left arm</customLabel>
                            <coverage>0.77</coverage>
                            <groups>
                                <li>Arms</li>
                            </groups>
                            <parts>
                                <!-- 左手 -->
                                <li>
                                    <def>Hand</def>
                                    <customLabel>left hand</customLabel>
                                    <coverage>0.14</coverage>
                                    <groups>
                                        <li>Hands</li>
                                    </groups>
                                    <parts>
                                        <!-- 左手手指 -->
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>left ring finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>left middle finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>left index finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>left pinky finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>left thumb</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                    </parts>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <!-- 右肩 -->
                <li>
                    <def>Shoulder</def>
                    <customLabel>right shoulder</customLabel>
                    <coverage>0.04</coverage>
                    <groups>
                        <li>Shoulders</li>
                    </groups>
                    <parts>
                        <!-- 右臂 -->
                        <li>
                            <def>Arm</def>
                            <customLabel>right arm</customLabel>
                            <coverage>0.77</coverage>
                            <groups>
                                <li>Arms</li>
                            </groups>
                            <parts>
                                <!-- 右手 -->
                                <li>
                                    <def>Hand</def>
                                    <customLabel>right hand</customLabel>
                                    <coverage>0.14</coverage>
                                    <groups>
                                        <li>Hands</li>
                                    </groups>
                                    <parts>
                                        <!-- 右手手指 -->
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>right ring finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>right middle finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>right index finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>right pinky finger</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                        <li>
                                            <def>Finger</def>
                                            <customLabel>right thumb</customLabel>
                                            <coverage>0.02</coverage>
                                            <groups>
                                                <li>Hands</li>
                                            </groups>
                                        </li>
                                    </parts>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <!-- 腰部 - conceptual part, coverage must be 0 -->
                <li>
                    <def>Waist</def>
                    <coverage>0</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
                <!-- 左腿 -->
                <li>
                    <def>Leg</def>
                    <customLabel>left leg</customLabel>
                    <coverage>0.08</coverage>
                    <groups>
                        <li>Legs</li>
                    </groups>
                    <parts>
                        <!-- 左脚 -->
                        <li>
                            <def>Foot</def>
                            <customLabel>left foot</customLabel>
                            <coverage>0.14</coverage>
                            <groups>
                                <li>Feet</li>
                            </groups>
                            <parts>
                                <!-- 左脚脚趾 -->
                                <li>
                                    <def>Toe</def>
                                    <customLabel>left big toe</customLabel>
                                    <coverage>0.02</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>left second toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>left middle toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>left fourth toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>left little toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <!-- 右腿 -->
                <li>
                    <def>Leg</def>
                    <customLabel>right leg</customLabel>
                    <coverage>0.08</coverage>
                    <groups>
                        <li>Legs</li>
                    </groups>
                    <parts>
                        <!-- 右脚 -->
                        <li>
                            <def>Foot</def>
                            <customLabel>right foot</customLabel>
                            <coverage>0.14</coverage>
                            <groups>
                                <li>Feet</li>
                            </groups>
                            <parts>
                                <!-- 右脚脚趾 -->
                                <li>
                                    <def>Toe</def>
                                    <customLabel>right big toe</customLabel>
                                    <coverage>0.02</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>right second toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>right middle toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>right fourth toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                                <li>
                                    <def>Toe</def>
                                    <customLabel>right little toe</customLabel>
                                    <coverage>0.01</coverage>
                                    <groups>
                                        <li>Feet</li>
                                    </groups>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <!-- 龙尾 -->
                <li>
                    <def>Sideria_DragonTail</def>
                    <coverage>0.05</coverage>
                    <groups>
                        <li>Torso</li>
                    </groups>
                </li>
            </parts>
        </corePart>
    </BodyDef>

    <!-- 龙角 - 自定义身体部位 -->
    <BodyPartDef>
        <defName>Sideria_DragonHorn</defName>
        <label>dragon horn</label>
        <hitPoints>15</hitPoints>
        <permanentInjuryChanceFactor>0</permanentInjuryChanceFactor>
        <bleedRate>0</bleedRate>
        <skinCovered>false</skinCovered>
        <solid>true</solid>
        <beautyRelated>true</beautyRelated>
        <canSuggestAmputation>false</canSuggestAmputation>
        <destroyableByDamage>false</destroyableByDamage>
    </BodyPartDef>

    <!-- 龙尾 - 自定义身体部位 -->
    <BodyPartDef>
        <defName>Sideria_DragonTail</defName>
        <label>dragon tail</label>
        <hitPoints>25</hitPoints>
        <permanentInjuryChanceFactor>0.2</permanentInjuryChanceFactor>
        <bleedRate>0.5</bleedRate>
        <skinCovered>true</skinCovered>
        <solid>false</solid>
        <beautyRelated>true</beautyRelated>
        <canSuggestAmputation>false</canSuggestAmputation>
        <destroyableByDamage>false</destroyableByDamage>
    </BodyPartDef>
</Defs>